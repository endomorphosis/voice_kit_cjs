{
  "name": "browser-extension",
  "version": "0.0.1",
  "description": "",
  "type": "module",
  "scripts": {
    "build": "npm run clean && webpack --mode production && npm run copy-wasm",
    "dev": "webpack --mode development --watch",
    "clean": "rimraf build",
    "copy-wasm": "mkdir -p build/wasm && cp node_modules/@huggingface/transformers/dist/*.wasm build/wasm/ && cp node_modules/onnxruntime-web/dist/*.wasm build/wasm/"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@huggingface/transformers": "^3.0.2",
    "onnxruntime-web": "^1.16.3"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-top-level-await": "^7.14.5",
    "@babel/preset-env": "^7.23.5",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^12.0.2",
    "html-webpack-plugin": "^5.6.0",
    "rimraf": "^5.0.5",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "browser": {
    "fs": false,
    "path": false,
    "crypto": false
  },
  "browserslist": [
    "chrome >= 113"
  ]
}
